constant symbol bvxnor [m : τ Int]: τ ((BitVec m) ⤳ (BitVec m) ⤳ BitVec m);
constant symbol bvcomp [m : τ Int]: τ ((BitVec m) ⤳ (BitVec m) ⤳ BitVec 1);

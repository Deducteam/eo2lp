require cpc.Prelude as eo;
require open cpc.theories.Ints cpc.programs.Arith;
constant symbol BitVec : τ (Int ⤳ eo.Type);
sequential symbol !bv_bitwidth : τ (eo.Type ⤳ Int);
rule !bv_bitwidth (BitVec $n) ↪ $n;
constant symbol bvsize [m : τ Int]: τ ((BitVec m) ⤳ Int);
symbol bv_empty  ≔ eo.to_bin 0 0;
constant symbol concat [n : τ Int] [m : τ Int]: τ ((BitVec n) ⤳ (BitVec m) ⤳ BitVec (eo.add n m));
constant symbol extract [n : τ Int] (h : τ Int) (l : τ Int): τ ((BitVec n) ⤳ BitVec (eo.requires (eo.gt (eo.add l 1) 0) true (eo.requires (eo.gt n h) true (eo.add (eo.add h (eo.neg l)) 1))));
constant symbol repeat [n : τ Int] (i : τ Int): τ ((BitVec n) ⤳ BitVec (eo.mul i n));
constant symbol bvnot [m : τ Int]: τ ((BitVec m) ⤳ BitVec m);
constant symbol bvand [m : τ Int]: τ ((BitVec m) ⤳ (BitVec m) ⤳ BitVec m);
constant symbol bvor [m : τ Int]: τ ((BitVec m) ⤳ (BitVec m) ⤳ BitVec m);
